{
    "name": "maintainerati/bikeshed-server",
    "description": "Server project template for the Maintainerati Bikeshed Open Space note taker",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "maintainerati/bikeshed-bundle": "dev-development@dev",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.x-dev@dev",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "camelot/coding-style": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.15",
        "psalm/plugin-phpunit": "^0.5.5",
        "symfony/debug-bundle": "^4.3",
        "symfony/http-client": "^4.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.3",
        "symfony/webpack-encore-bundle": "^1.5",
        "vimeo/psalm": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "Maintainerati\\Bikeshed\\Util\\AssetInstall::run"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "Maintainerati\\Bikeshed\\Util\\AssetInstall::run"
        ],
        "lint": "vendor/bin/php-cs-fixer fix --show-progress=dots -v",
        "static": "vendor/bin/psalm --find-dead-code",
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit --coverage-text",
        "suite": [
            "@lint",
            "@static",
            "@coverage"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.3"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/maintainerati/bikeshed.git"
        },
        {
            "type": "git",
            "url": "https://github.com/maintainerati/bikeshed-bundle.git"
        }
    ]
}
